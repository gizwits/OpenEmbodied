{
    "target": "esp32c2",
    "builds": [
        {
            "name": "doit-ai-01-kit",
            "sdkconfig_append": [
                "CONFIG_BOARD_TYPE_DOIT_AI_01_KIT=y",
                "CONFIG_MQTT_USE_CUSTOM_CONFIG=y",
                "CONFIG_ESPTOOLPY_FLASHSIZE_4MB=n",
                "CONFIG_PARTITION_TABLE_CUSTOM_FILENAME=partitions_4M.csv",
                "CONFIG_PARTITION_TABLE_FILENAME=partitions_4M.csv",
                "CONFIG_ESP32C2_REV_MIN_200=200",
                "CONFIG_ESP32C2_REV_MIN_FULL=200",
                "CONFIG_ESP_REV_MIN_FULL=200",
                "CONFIG_USE_EYE_STYLE_VB6824=y",
                "CONFIG_USE_AUDIO_PROCESSOR=n"
            ]
        }
    ]
}


